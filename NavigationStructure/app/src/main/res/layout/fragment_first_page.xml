<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FirstPageFragment">


    <Button
        android:id="@+id/goToHelp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="gotohelp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/goToContact" />

    <Button
        android:id="@+id/goToContact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GoToConctact"
        app:layout_constraintBottom_toTopOf="@+id/goToHelp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/goToSearch" />

    <Button
        android:id="@+id/goToSearch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GoToSearch"
        app:layout_constraintBottom_toTopOf="@+id/goToContact"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>